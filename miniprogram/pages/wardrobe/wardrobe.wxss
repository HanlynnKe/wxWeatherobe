/* pages/wardrobe/wardrobe.wxss */
page {
  background: #f6f6f6;
  width: 100%;
  height: 100%;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.flex-content-center {
  height: 100%;
  width: 50%;
  text-align: center;
  justify-content: center;
  justify-items: center;
}

.num-content {
  margin-top: 0rpx;
  padding-left: 30rpx;
}

.user-num {
  display: inline-block;
  font-size: 300rpx;
  color: white;
  vertical-align: top;
  padding: 0;
}

.user-num-bottom {
  text-align: center;
  font-size: 36rpx;
  color: #f5f6fa;
}

.num-content__bottom {
  display: flex;
  justify-content: center;
  flex-direction: column;
  height: 170rpx;
}

.upload-btn {
  display: flex;
  width: 450rpx;
  margin-top: 60rpx;
}

.delete-btn {
  display: flex;
  width: 450rpx;
  margin-top: 30rpx;
}

.image-go {
 display: block;
 width: 100%;
 height: 70rpx;
 background-color: #f2f2f2;
 line-height: 70rpx;
}

.marquee-box {
 width: 100%;
 position: relative;
}

.marquee-text {
 white-space: nowrap;
 position: absolute;
 top: 0;
 display: flex;
 flex-direction: row;
}

.ad-image {
 width: 40rpx;
 height: 40rpx;
 margin-right: 10rpx;
 margin-top: 15rpx;
}

